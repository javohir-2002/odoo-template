<!-- room_image_views.xml -->
<odoo>
	<data>

		<!-- Tree View -->
		<record id="view_basetype_tree" model="ir.ui.view">
			<field name="name">accommodation.basetype.tree</field>
			<field name="model">base.types</field>
			<field name="arch" type="xml">
				<tree string="Building type">
					<field name="name" />
				</tree>
			</field>
		</record>

		<!-- Form View -->
		<record id="view_basetype_form" model="ir.ui.view">
			<field name="name">accommodation.basetype.form</field>
			<field name="model">base.types</field>
			<field name="arch" type="xml">
				<form string="Building type">
					<sheet>
						<group>
							<field name="name" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

	</data>
</odoo>
  