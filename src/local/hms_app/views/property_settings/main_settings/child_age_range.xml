<!-- room_image_views.xml -->
<odoo>
	<data>

		<!-- Tree View -->
		<record id="view_childage_tree" model="ir.ui.view">
			<field name="name">accommodation.childage.tree</field>
			<field name="model">child.age.range</field>
			<field name="arch" type="xml">
				<tree string="Child ages">
					<field name="age_from" />
					<field name="age_to" />
					<field name="room_type" />
				</tree>
			</field>
		</record>

		<!-- Form View -->
		<record id="view_childage_form" model="ir.ui.view">
			<field name="name">accommodation.childage.form</field>
			<field name="model">child.age.range</field>
			<field name="arch" type="xml">
				<form string="Child ages">
					<sheet>
						<group>
							<field name="age_from" />
							<field name="age_to" />
							<field name="room_type" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</odoo>
  