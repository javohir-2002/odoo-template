<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Tree View -->
        <record id="view_services_motivator_tree" model="ir.ui.view">
            <field name="name">motivator.booking.engine.tree</field>
            <field name="model">motivator.booking.engine</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>

        <!-- Form View -->
        <record id="view_motivator_form" model="ir.ui.view">
            <field name="name">motivator.booking.engine.form</field>
            <field name="model">motivator.booking.engine</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>