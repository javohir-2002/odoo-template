<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_transfer_type_tree" model="ir.ui.view">
        <field name="name">transfer.type.tree</field>
        <field name="model">transfer.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="view_transfer_type_form" model="ir.ui.view">
        <field name="name">transfer.type.form</field>
        <field name="model">transfer.type</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name" />
                </group>
            </form>
        </field>
    </record>
</odoo>