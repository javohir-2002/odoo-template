<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_station_tree" model="ir.ui.view">
        <field name="name">station.tree</field>
        <field name="model">station</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="transfer_type_id" />
            </tree>
        </field>
    </record>

    <record id="view_station_form" model="ir.ui.view">
        <field name="name">station.form</field>
        <field name="model">station</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name" />
                    <field name="transfer_type_id" />
                </group>
            </form>
        </field>
    </record>
</odoo>