<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="view_promo_code_valid_date_tree" model="ir.ui.view">
        <field name="name">promo.code.valid.date.tree</field>
        <field name="model">property_settings.promo_code.valid_date</field>
        <field name="arch" type="xml">
            <tree>
                <field name="date_from" />
                <field name="date_to" />
                <!-- <field name="promocode_id" /> -->
            </tree>
        </field>
    </record>

    <record id="view_promo_code_valid_date_form" model="ir.ui.view">
        <field name="name">promo.code.valid.date.form</field>
        <field name="model">property_settings.promo_code.valid_date</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="date_from" />
                    <field name="date_to" />
                    <!-- <field name="promocode_id" /> -->
                </group>
            </form>
        </field>
    </record>
</odoo>