<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_all_times_tree" model="ir.ui.view">
        <field name="name">all.times.tree</field>
        <field name="model">all.times</field>
        <field name="arch" type="xml">
            <tree string="Time">
                <field name="time" widget="float_time" />
            </tree>
        </field>
    </record>

    <record id="view_all_times_form" model="ir.ui.view">
        <field name="name">all.times.form</field>
        <field name="model">all.times</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="time" widget="float_time" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>