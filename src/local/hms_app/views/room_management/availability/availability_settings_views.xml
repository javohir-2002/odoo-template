<odoo>
    <record id="view_advanced_availability_settings_tree" model="ir.ui.view">
        <field name="name">availability_settings.tree</field>
        <field name="model">availability_settings</field>
        <field name="arch" type="xml">
            <tree js_class="AvailabilitySettings">
                <field name="date" />
                <field name="role_sales" />
            </tree>
        </field>
    </record>

    <record id="view_advanced_availability_settings_form" model="ir.ui.view">
        <field name="name">availability_settings.form</field>
        <field name="model">availability_settings</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="date" />
                        <field name="role_sales" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
