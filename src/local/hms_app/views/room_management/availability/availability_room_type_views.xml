<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="view_availability_room_type_tree" model="ir.ui.view">
        <field name="name">availability.room_type.tree</field>
        <field name="model">availability.room_type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="room_type_id" />
                <field name="number" />
            </tree>
        </field>
    </record>
    <record id="view_availability_room_type_form" model="ir.ui.view">
        <field name="name">availability.room_type.form</field>
        <field name="model">availability.room_type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="room_type_id" />
                        <field name="number" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
