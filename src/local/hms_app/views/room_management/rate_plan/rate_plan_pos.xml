<!-- accommodation_offer_views.xml -->
<odoo>
	<data>

		<!-- Tree View for Smoking In Room -->
		<record id="rate_plan_pos_tree" model="ir.ui.view">
			<field name="name">postree</field>
			<field name="model">point.of.sale</field>
			<field name="arch" type="xml">
				<tree string="Point of sales">
					<field name="name" />
				</tree>
			</field>
		</record>

		<!-- Form View for Smoking In Room -->
		<record id="rate_plan_pos_form" model="ir.ui.view">
			<field name="name">posform</field>
			<field name="model">point.of.sale</field>
			<field name="arch" type="xml">
				<form string="Point of sale">
					<sheet>
						<group>
							<field name="name" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

	</data>
</odoo>
  