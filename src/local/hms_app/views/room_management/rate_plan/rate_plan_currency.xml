<!-- accommodation_offer_views.xml -->
<odoo>
	<data>

		<!-- Tree View for Smoking In Room -->
		<record id="rate_plan_curr_view_tree" model="ir.ui.view">
			<field name="name">currtree</field>
			<field name="model">currency</field>
			<field name="arch" type="xml">
				<tree string="Rate plan currencies">
					<field name="currency" />
				</tree>
			</field>
		</record>

		<!-- Form View for Smoking In Room -->
		<record id="rate_plan_curr_view_form" model="ir.ui.view">
			<field name="name">currform</field>
			<field name="model">currency</field>
			<field name="arch" type="xml">
				<form string="Rate plan currency">
					<sheet>
						<group>
							<field name="currency" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</odoo>
  