<!-- accommodation_offer_views.xml -->
<odoo>
	<data>

		<!-- Tree View for Smoking In Room -->
		<record id="rate_plan_cancel_tree" model="ir.ui.view">
			<field name="name">canceltree</field>
			<field name="model">cancel.rule</field>
			<field name="arch" type="xml">
				<tree string="Cancellation policy">
					<field name="name" />
				</tree>
			</field>
		</record>

		<!-- Form View for Smoking In Room -->
		<record id="rate_plan_cancel_form" model="ir.ui.view">
			<field name="name">cancelform</field>
			<field name="model">cancel.rule</field>
			<field name="arch" type="xml">
				<form string="Cancellation rule setting">
					<sheet>
						<group>
							<field name="name" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</odoo>
  