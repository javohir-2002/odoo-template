<odoo>
	<!-- Delete -->
	<record id="view_calendar_close_data" model="ir.ui.view">
		<field name="name">close.data.calendar</field>
		<field name="model">close</field>
		<field name="arch" type="xml">
			<calendar string="Room Type Calendar" date_start="date" color="open_close"
				quick_create="false" event_open_popup="true" mode="month">
				<field name="status_display" invisible="1" filters="1" />
				<field name="room_type_id" />
				<field name="date" />
				<field name="child_aged_from_price" />
				<field name="single_room_price" />
				<field name="double_room_price" />
				<field name="triple_room_price" />
				<field name="fourth_room_price" />
				<field name="fifth_room_price" />
				<field name="sixth_room_price" />
				<field name="seventh_room_price" />
			</calendar>


		</field>
	</record>

	<record id="action_calendar_close_data" model="ir.actions.act_window">
		<field name="name">Room Type Calendar</field>
		<field name="res_model">close</field>
		<field name="view_mode">calendar</field>
		<field name="view_id" ref="view_calendar_close_data" />
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Schedule room type availability here.
			</p>
		</field>
	</record>

</odoo>