<!-- accommodation_offer_views.xml -->
<odoo>
	<data>

		<!-- Tree View for Smoking In Room -->
		<record id="rate_plan_check_tree" model="ir.ui.view">
			<field name="name">checktree</field>
			<field name="model">checkinout</field>
			<field name="arch" type="xml">
				<tree string="Early Check-In and Late Check-Out Rule">
					<field name="name" />
				</tree>
			</field>
		</record>

		<!-- Form View for Smoking In Room -->
		<record id="rate_plan_cancel_form" model="ir.ui.view">
			<field name="name">cancelform</field>
			<field name="model">checkinout</field>
			<field name="arch" type="xml">
				<form string="Early Check-In and Late Check-Out Rule">
					<sheet>
						<group>
							<field name="name" />
						</group>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</odoo>
  