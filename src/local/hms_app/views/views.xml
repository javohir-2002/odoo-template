<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="hms_app.list">
      <field name="name">hms_app list</field>
      <field name="model">hms_app.hms_app</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name" />
          <field name="value" />
          <field name="value2" />
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="hms_app.action_window">
      <field name="name">hms_app window</field>
      <field name="res_model">hms_app.hms_app</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="hms_app.action_server">
      <field name="name">hms_app server</field>
      <field name="model_id" ref="model_hms_app_hms_app" />
      <field name="state">code</field>
      <field name="code">
        action = {
        "type": "ir.actions.act_window",
        "view_mode": "tree,form",
        "res_model": model._name,
        }
      </field>
    </record>


