<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_accommodation_tree" model="ir.ui.view">
        <field name="name">acommodation.tree</field>
        <field name="model">accommodation</field>
        <field name="arch" type="xml">
              <tree string="Accommodation">
                <field name="name"/>
                <field name="check_in_date"/>
                <field name="check_out_date"/>
                <field name="rate_plan"/>
                <field name="room_type"/>
                <field name="adults"/>
                <field name="price"/>
                <field name="vacant"/>
                <field name="booking_unique"/>
            </tree>
        </field>
    </record>
</odoo>
