<odoo>
    <record id="reference_point_of_sale" model="references">
        <field name="reference_name">Point of sale</field>
        <field name="default_element">via front desk</field>
        <field name="number_of_elements">12</field>
        <field name="date_last_modified">2024-07-26 22:14:00</field>
        <field name="modified_by_user_id" ref="base.user_admin"/>
    </record>
    <record id="reference_purpose_of_visit" model="references">
        <field name="reference_name">Purpose of visit</field>
        <field name="default_element">Tourism</field>
        <field name="number_of_elements">10</field>
        <field name="modified_by_user_id" ref="base.user_admin"/>
    </record>
    <record id="reference_tag" model="references">
        <field name="reference_name">Tag</field>
        <field name="number_of_elements">2</field>
        <field name="modified_by_user_id" ref="base.user_admin"/>
    </record>
    <record id="reference_payment_method" model="references">
        <field name="reference_name">Payment method</field>
        <field name="default_element">By cash via cashbox</field>
        <field name="number_of_elements">7</field>
        <field name="date_last_modified">2023-12-28 14:31:00</field>
        <field name="modified_by_user_id" ref="base.user_admin"/>
    </record>
    <record id="reference_market_segment" model="references">
        <field name="reference_name">Market segment</field>
        <field name="default_element">Individual</field>
        <field name="number_of_elements">8</field>
        <field name="modified_by_user_id" ref="base.user_admin"/>
    </record>
    <record id="reference_staff" model="references">
        <field name="reference_name">Staff</field>
        <field name="number_of_elements">0</field>
        <field name="modified_by_user_id" ref="base.user_admin"/>
    </record>
    <record id="reference_guest_status" model="references">
        <field name="reference_name">Guest status</field>
        <field name="default_element">Not selected</field>
        <field name="number_of_elements">5</field>
        <field name="modified_by_user_id" ref="base.user_admin"/>
    </record>

    <!-- Default elements for Point of Sale -->
    <record id="element_via_front_desk" model="references.element">
        <field name="name">via front desk</field>
        <field name="element_status">active</field>
        <field name="element_modified_by_user_id" ref="base.user_admin"/>
    </record>
    <record id="element_via_phone" model="references.element">
        <field name="name">via phone</field>
        <field name="element_status">active</field>
        <field name="element_modified_by_user_id" ref="base.user_admin"/>
    </record>
</odoo>
